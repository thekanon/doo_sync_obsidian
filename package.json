{
  "name": "doo_sync_obsidian",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 33000",
    "build": "next build",
    "start": "next start -p 33000",
    "lint": "next lint",
    "clean": "rimraf dist coverage .nyc_output",
    "build:test": "tsc -p tsconfig.test.json && tsc-alias -p tsconfig.test.json",
    "pretest": "npm run build:test",
    "test": "node --test --import ./dist/__tests__/setup.js \"dist/__tests__/**/*.test.js\"",
    "pretest:unit": "npm run build:test",
    "test:unit": "node --test --import ./dist/__tests__/setup.js \"dist/__tests__/unit/**/*.test.js\"",
    "pretest:integration": "npm run build:test",
    "test:integration": "node --test --import ./dist/__tests__/setup.js \"dist/__tests__/integration/**/*.test.js\"",
    "pretest:components": "npm run build:test",
    "test:components": "node --test --import ./dist/__tests__/setup.js \"dist/__tests__/components/**/*.test.js\"",
    "pretest:all": "npm run build:test",
    "test:all": "node --test --import ./dist/__tests__/setup.js \"dist/__tests__/**/*.test.js\"",
    "pretest:coverage": "npm run build:test",
    "test:coverage": "c8 -r text -r html -r lcov --all --include \"app/**/*.ts\" --include \"services/**/*.ts\" npm run test:all",
    "test:watch": "npm run build:test && node --test --watch --import ./dist/__tests__/setup.js \"dist/__tests__/**/*.test.js\"",
    "test:jest": "jest",
    "test:jest-coverage": "jest --coverage",
    "test:nyc": "nyc --include=\"app/**/*.js\" --include=\"services/**/*.js\" --exclude=\"**/*.test.js\" --exclude=\"**/__tests__/**\" node --test \"dist/__tests__/**/*.test.js\""
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.15",
    "@types/tailwindcss": "^3.1.0",
    "cookie": "^1.0.1",
    "crypto": "^1.0.1",
    "dompurify": "^3.1.6",
    "es-toolkit": "^1.27.0",
    "firebase": "^10.14.0",
    "firebase-admin": "^12.6.0",
    "firebaseui": "^6.1.0",
    "geist": "^1.3.1",
    "isomorphic-dompurify": "^2.26.0",
    "jsdom": "^25.0.0",
    "lucide-react": "^0.459.0",
    "marked": "^14.1.2",
    "next": "14.2.11",
    "node-html-parser": "^6.1.13",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/cookie": "^0.6.0",
    "@types/dompurify": "^3.0.5",
    "@types/jest": "^30.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "c8": "^10.1.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.11",
    "jest": "^30.0.5",
    "nyc": "^17.1.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.4.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5"
  },
  "packageManager": "pnpm@10.8.1+sha512.c50088ba998c67b8ca8c99df8a5e02fd2ae2e2b29aaf238feaa9e124248d3f48f9fb6db2424949ff901cffbb5e0f0cc1ad6aedb602cd29450751d11c35023677"
}
